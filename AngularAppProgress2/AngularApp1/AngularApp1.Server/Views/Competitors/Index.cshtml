@model IEnumerable<AngularApp1.Server.Models.Competitor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Pid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.schoolName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BarPos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BeamPos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FloorPos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VaultPos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sub)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MeetId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.schoolName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BarPos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BeamPos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FloorPos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VaultPos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sub)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MeetId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
